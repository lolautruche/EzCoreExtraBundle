{
    "name": "lolautruche/ez-core-extra-bundle",
    "description": "Extra sugar for building eZ Platform websites",
    "require": {
        "ezsystems/ezpublish-kernel": "^8.0",
        "twig/twig": "^3.0",
        "symfony/config": "^5.0",
        "symfony/event-dispatcher": "^5.0",
        "symfony/dependency-injection": "^5.0",
        "symfony/security-core": "^5.0",
        "symfony/http-foundation": "^5.0",
        "symfony/http-kernel": "^5.0",
        "symfony/expression-language": "^5.0",
        "symfony/options-resolver": "^5.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Jerome Vieilledent",
            "email": "lolautruche@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {"Lolautruche\\EzCoreExtraBundle\\": ""}
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mikey179/vfsstream": "^1.6.3"
    },
    "suggest": {
        "ezsystems/ezplatform-design-engine": "Design fallback mechanism for eZ Platform (previously part of EzCoreExtraBundle)"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
